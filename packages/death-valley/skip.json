{
  "name": "death-valley",
  "version": "0.1.0",
  "description": "Lovefield-TS: a relational database in TypeScript.",
  "author": "Arthur Hsu",
  "contributors": [
    {
      "name": "Brian Jenkins"
    }
  ],
  "license": "Apache-2.0",
  "main": "deathValley.ts",
  "types": "deathValley.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run build --watch",
    "build": "tsup deathValley.ts --format esm --dts",
    "test": "node --experimental-specifier-resolution=node --loader=ts-node/esm test.ts"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^7.32.0",
    "ts-node": "latest",
    "tsup": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/deathValley.js",
      "types": "./dist/deathValley.d.ts"
    }
  },
  "files": [
    "dist/"
  ]
}
