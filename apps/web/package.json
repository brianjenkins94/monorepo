{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "cypress": "CODE=0 ; (npm start & { npx cypress run ; CODE=$? ; kill -INT $(jobs -p) ; }) ; (exit $CODE)",
    "cypress-static": "CODE=0 ; npm run build && (npm run serve & { npx cypress run --config baseUrl=http://localhost:8080/ ; CODE=$? ; kill -INT $(jobs -p) ; }) ; (exit $CODE)",
    "build": "next build # && npm run postbuild && next export && npm run postexport",
    "postbuild": "node --experimental-specifier-resolution=node --loader=ts-node/esm scripts/postbuild.ts",
    "postexport": "node --experimental-specifier-resolution=node --loader=ts-node/esm scripts/postexport.ts",
    "purge": "rm -rf .next out package-lock.json",
    "serve": "npx http-server . -c-1",
    "start": "node --experimental-specifier-resolution=node --loader=ts-node/esm server.ts"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "devDependencies": {
    "@code-hike/mdx": "latest",
    "@codesandbox/sandpack-react": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "bottleneck": "latest",
    "cypress": "latest",
    "esbuild": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "mdx-bundler": "latest",
    "mssql": "latest",
    "msw": "latest",
    "next-transpile-modules": "latest",
    "next": "latest",
    "nextra-theme-docs": "beta",
    "nextra": "beta",
    "playwright-chromium": "latest",
    "react-dom": "latest",
    "react": "latest",
    "sass": "latest",
    "saxes": "latest",
    "ts-node": "latest",
    "turndown": "latest",
    "turndown-plugin-gfm": "npm:joplin-turndown-plugin-gfm@latest",
    "typescript": "latest"
  }
}
