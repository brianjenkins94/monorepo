{
  "name": "ratatouille",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjenkins94/ratatouille.git"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianjenkins94/ratatouille/issues"
  },
  "homepage": "https://github.com/brianjenkins94/ratatouille#readme",
  "scripts": {
    "build": "next build --no-lint && next export && npm run postexport",
    "postexport": "find out -name \"*.html\" -exec sed -i \"s/\\\"\\/_next\\//\\\"\\/ratatouille\\/_next\\//g\" \"{}\" \\;",
    "publish": "npm run build && git clone https://github.com/brianjenkins94/ratatouille.git && rsync --exclude=\".git/\" --exclude=\"ratatouille/\" --recursive ./ ratatouille && sed \"s/monorepo/ratatouille/\" ../../.git/config > ratatouille/.git/config && cd ratatouille && rm -r docs && mv out docs && git add --all && git commit --message \"Automated build\" && git push origin main",
    "start": "next dev"
  },
  "dependencies": {
    "cheerio": "latest",
    "fido": "*",
    "next": "latest",
    "playwright-chromium": "latest",
    "react-dom": "^18",
    "react": "^18"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react-dom": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-next": "latest",
    "eslint": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "sideEffects": false
}
